{
  "extends": [
    "standard",
    "plugin:react/recommended",
    "plugin:jest/recommended",
    "plugin:prettier/recommended",
    "plugin:css-modules/recommended",
    "prettier/react",
    "prettier/standard"
  ],
  "plugins": [
    "react",
    "standard",
    "jest",
    "css-modules"
  ],
  "parserOptions": {
