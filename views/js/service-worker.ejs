<%- serviceWorkerHelper %>;

const ISOMORPHIC_ROUTES = <%- JSON.stringify(routes) %>;
const REQUIRED_ASSETS = [
  '/shell.html',
  <%_ const cssPath = assetPath(["app", "css"]) _%>
  <%_ if(cssPath) { _%>
  "<%= cssPath %>",
  <%_ } _%>
  "<%= assetPath(["app", "js"]) %>"
];

initializeQServiceWorker(self, {
  routes: ISOMORPHIC_ROUTES,
  assets: REQUIRED_ASSETS
});
